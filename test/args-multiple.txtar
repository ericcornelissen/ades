! exec ades project-a project-b
stdout 'project-a'
stdout 'project-b'
stdout '.github/workflows/unsafe.yml'
! stderr .


-- project-a/.github/workflows/safe.yml --
name: Example safe workflow
on: [push]

jobs:
  example:
    name: Example safe job
    runs-on: ubuntu-latest
    steps:
    - name: Checkout repository
      uses: actions/checkout@v3
    - name: Safe run
      run: echo 'Hello world!'
-- project-b/.github/workflows/unsafe.yml --
name: Example unsafe workflow
on: [push]

jobs:
  example:
    name: Example unsafe job
    runs-on: ubuntu-latest
    steps:
    - name: Checkout repository
      uses: actions/checkout@v3
    - name: Unsafe run
      run: echo 'Hello ${{ inputs.name }}'
